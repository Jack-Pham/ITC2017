<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>ITC</title>

    <!-- Bootstrap -->
    <link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <script src="/jquery/jquery.min.js"></script>
    <link href="/bootstrap/js/bootstrap.min.js" rel="stylesheet">

    <!-- Custom Stylesheets -->
    <link href="/css/map.css" rel="stylesheet">

    <!-- Custom Scripts -->
    <script src="/js/scripts.js"></script>

    <!-- Google API-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAt_AXYXGn0Xj16bLgiMr6TXAn7WUX24IM&libraries=places,drawing&callback=initMap" async defer></script>
</head>
<body>

<!--Search Bar-->
<div id="Menu-bar">
    <input id="autocomplete" class="controls" type="text" placeholder="Enter Place">
</div>
<table border=1>
    <tr>
        <td>
            <div id="map"></div>
        </td>
        <td>
            <div id="side_bar">
                <div id="places">
                </div>
                <div id="ratings">
                    <p>Rating Level</p>
                    <a onclick="sort_by_rating(1);" class="btn" role="button">1+</a>
                    <a onclick="sort_by_rating(2);" class="btn" role="button">2+</a>
                    <a onclick="sort_by_rating(3);" class="btn" role="button">3+</a>
                    <a onclick="sort_by_rating(4);" class="btn" role="button">4+</a>
                    <a onclick="sort_by_rating(0);" class="btn" role="button">Any</a>
                </div>
                <div id="buttons">
                    <p>Price Level</p>
                    <a onclick="sort_by_price(1);" class="btn" role="button">1</a>
                    <a onclick="sort_by_price(2);" class="btn" role="button">2</a>
                    <a onclick="sort_by_price(3);" class="btn" role="button">3</a>
                    <a onclick="sort_by_price(4);" class="btn" role="button">4</a>
                    <a onclick="sort_by_price(0);" class="btn" role="button">Any</a>
                </div>
            </div>
        </td>
    </tr>
</table>
</body>
</html>